using System;

namespace Homework_2
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int minValue = 7;
            int maxValue = 98;
            int step = 7;
            for (int i = minValue; i <= maxValue; i += step)
            {
                Console.Write(i + " ");
            }
        }
    }
}
