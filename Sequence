using System;

namespace Homework_2
{
    internal class StringsPractice
    {
        static void Main(string[] args)
        {
            float output;
            float remainder;
            for (int i=1; i<100; i++)
            {
            remainder = i%7;
            if (remainder == 0)
            {
            output = i;
            Console.Write(output + " ");            
            }
        }
    }
}
